<div class="page-header">
  <%= link_to examenes_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_examen_path(@examen), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show examen</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nombre:</dt>
  <dd><%= @examen.nombre %></dd>

  <dt>Descripcion:</dt>
  <dd><%= @examen.descripcion %></dd>

</dl>
