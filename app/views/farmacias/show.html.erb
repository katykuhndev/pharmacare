<div class="page-header">
  <%= link_to farmacias_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_farmacia_path(@farmacia), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show farmacia</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nombre:</dt>
  <dd><%= @farmacia.nombre %></dd>

  <dt>Contacto:</dt>
  <dd><%= @farmacia.contacto %></dd>

  <dt>Fono:</dt>
  <dd><%= @farmacia.fono %></dd>

  <dt>Direccion:</dt>
  <dd><%= @farmacia.direccion %></dd>

  <dt>Comuna:</dt>
  <dd><%= @farmacia.comuna %></dd>

  <dt>Tipo:</dt>
  <dd><%= @farmacia.tipo %></dd>

</dl>
