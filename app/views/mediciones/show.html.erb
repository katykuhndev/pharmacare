<div class="page-header">
  <%= link_to mediciones_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_medicion_path(@medicion), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show medicion</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nombre:</dt>
  <dd><%= @medicion.nombre %></dd>

  <dt>Descripcion:</dt>
  <dd><%= @medicion.descripcion %></dd>

  <dt>Programa:</dt>
  <dd><%= @medicion.programa %></dd>

  <dt>Examen:</dt>
  <dd><%= @medicion.examen %></dd>

  <dt>Unidad medida:</dt>
  <dd><%= @medicion.unidad_medida %></dd>

</dl>
