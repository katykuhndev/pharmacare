<div class="page-header">
  <%= link_to prestadores_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Volver
  <% end %>
  <%= link_to edit_prestador_path(@prestador), class: 'btn btn-default bg-secondary text-white' do %>
    <span class="glyphicon glyphicon-plus"></span>
    Editar
  <% end %>
  <h1>Prestador</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nombre:</dt>
  <dd><%= @prestador.nombre %></dd>

  <dt>Contacto:</dt>
  <dd><%= @prestador.contacto %></dd>

  <dt>Fono:</dt>
  <dd><%= @prestador.fono %></dd>

  <dt>Direccion:</dt>
  <dd><%= @prestador.direccion %></dd>

  <dt>Comuna:</dt>
  <dd><%= @prestador.comuna ? @prestador.comuna.nombre : '' %></dd>

</dl>
