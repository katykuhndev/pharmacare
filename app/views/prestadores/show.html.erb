<div class="page-header">
  <%= link_to prestadores_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_prestador_path(@prestador), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show prestador</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nombre:</dt>
  <dd><%= @prestador.nombre %></dd>

  <dt>Contacto:</dt>
  <dd><%= @prestador.contacto %></dd>

  <dt>Fono:</dt>
  <dd><%= @prestador.fono %></dd>

  <dt>Direccion:</dt>
  <dd><%= @prestador.direccion %></dd>

  <dt>Comuna:</dt>
  <dd><%= @prestador.comuna %></dd>

</dl>
